{
  "name": "geostyler-geojson-parser",
  "version": "0.4.6",
  "description": "GeoStyler Data Parser implementation for GeoJSON",
  "main": "build/dist/GeoJsonDataParser.js",
  "files": [
    "build",
    "index.d.ts",
    "browser"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/terrestris/geostyler-geojson-parser.git"
  },
  "keywords": [
    "geostyler",
    "parser",
    "data",
    "geojson"
  ],
  "author": "",
  "license": "BSD-2-Clause",
  "bugs": {
    "url": "https://github.com/terrestris/geostyler-geojson-parser/issues"
  },
  "homepage": "https://github.com/terrestris/geostyler-geojson-parser#readme",
  "dependencies": {
    "geostyler-data": "0.5.3"
  },
  "scripts": {
    "build": "tsc -p tsconfig.json && npm run build:browser",
    "build:browser": "webpack --config browser-build.config.js",
    "pretest": "npm run lint",
    "test": "jest",
    "lint": "tslint --project tsconfig.json --config tslint.json",
    "release": "np --no-yarn && git push https://github.com/terrestris/geostyler-geojson-parser.git master --tags"
  },
  "devDependencies": {
    "@babel/polyfill": "7.2.5",
    "@types/geojson": "7946.0.5",
    "@types/jest": "24.0.0",
    "@types/json-schema": "7.0.2",
    "@types/node": "10.12.19",
    "awesome-typescript-loader": "4.0.1",
    "coveralls": "3.0.1",
    "jest": "23.1.0",
    "np": "4.0.2",
    "ts-jest": "23.10.5",
    "tslint": "5.10.0",
    "typescript": "2.9.1",
    "uglifyjs-webpack-plugin": "2.1.1",
    "webpack": "4.29.0",
    "webpack-cli": "3.2.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "\\.(ts)$": "ts-jest"
    },
    "testRegex": "/src/.*\\.spec.(ts|js)$",
    "collectCoverageFrom": [
      "src/*.{ts}"
    ]
  }
}
